# Example environment variables for production. Copy to .env and fill with real secrets.
SECRET_KEY=change-me-to-a-long-secret
JWT_SECRET_KEY=change-me-to-a-different-long-secret
# Use a production DB URL, e.g. postgres://user:pass@host:5432/dbname
DATABASE_URL=sqlite:///phishing_detector.db
# Frontend build-time API base URL (used by Vite). Set this in Vercel as VITE_API_BASE
VITE_API_BASE=http://localhost:5000
# DEBUG should be False in production
DEBUG=False
